{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
            "distDir": "dist"
            }
        }
        ],
        "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/main.js"
        }
        ]
    }
    